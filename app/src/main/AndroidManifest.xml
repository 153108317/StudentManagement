<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.my.studenmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.my.studenmanagement.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.my.studenmanagement.activity.AdminLoginActivity"></activity>
        <activity android:name="com.example.my.studenmanagement.activity.StudentLoginActivity"></activity>
        <activity
            android:name="com.example.my.studenmanagement.activity.AdminActivity"
            android:launchMode="singleTask"></activity>
        <activity android:name="com.example.my.studenmanagement.activity.StudentInfoActivity"></activity>
        <activity android:name="com.example.my.studenmanagement.activity.AddStudentInfoActivity"></activity>
        <activity android:name="com.example.my.studenmanagement.activity.StudentTotalScore"></activity>
        <activity android:name=".activity.student_activity"></activity>
        <activity android:name="com.example.my.studenmanagement.activity.StudentActivity" />
        <activity android:name="com.example.my.studenmanagement.activity.TecherActivity" android:launchMode="singleTask"></activity>
        <activity android:name="com.example.my.studenmanagement.activity.TecherLoginActivity" />
        <activity android:name="com.example.my.studenmanagement.activity.AddTecherActivity" />
        <activity android:name="com.example.my.studenmanagement.activity.TecherInfoActivity" />

        <receiver android:name="com.example.my.studenmanagement.tools.MyBroadcastBestPractice">
            <intent-filter>
                <action android:name="com.he.example.OfflineBradcast" />
            </intent-filter>
        </receiver>
    </application>

</manifest>
